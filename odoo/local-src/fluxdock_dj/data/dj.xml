<odoo>
  <record model="dj.compilation" id="dj_fluxdock">
    <field name="name">Fluxdock</field>
    <field name="data_mode">sample</field>
    <field name="genre_id" ref="dj_core.dj_custom_genre" />
  </record>

  <record model="dj.song" id="dj_song_users">
    <field name="compilation_id" ref="dj_fluxdock"/>
    <field name="model_id" ref="base.model_res_users" />
    <field name="sequence" eval="10" />
    <field name="song_type">load_csv</field>
    <field name="field_list">
      name,active,company_id,digest_frequency,digest_mode,
      groups_id,login,notification_type,password
    </field>
  </record>

  <record model="dj.song" id="dj_song_professions">
    <field name="compilation_id" ref="dj_fluxdock"/>
    <field name="model_id" ref="project_profession.model_project_partner_profession" />
    <field name="sequence" eval="20" />
    <field name="song_type">load_csv</field>
  </record>

  <record model="dj.song" id="dj_song_proposal">
    <field name="compilation_id" ref="dj_fluxdock"/>
    <field name="model_id" ref="fluxdock_project.model_project_proposal" />
    <field name="sequence" eval="30" />
    <field name="song_type">load_csv</field>
    <field name="model_fields_blacklist_ids" eval="[(6,0,[
      ref('fluxdock_project.field_project_proposal_notify_dirty')])]"/>
  </record>

  <record model="dj.song" id="dj_song_reference">
    <field name="compilation_id" ref="dj_fluxdock"/>
    <field name="model_id" ref="fluxdock_project.model_project_reference" />
    <field name="sequence" eval="40" />
    <field name="song_type">load_csv</field>
  </record>

</odoo>
