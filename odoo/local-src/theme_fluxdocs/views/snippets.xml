	        </xpath>
	        
	        <xpath expr="//div[@id='snippet_structure']/div[@class='o_panel_body']" position="replace">
	            <!-- Remove all Snippets -->
	            <div class="o_panel_body"></div>
	        </xpath>
	        
	        <xpath expr="//div[@id='snippet_effect']/div[@class='o_panel_body']" position="replace">
	            <!-- Remove all Snippets -->
	            <div class="o_panel_body"></div>
	        </xpath>
	        
	        
