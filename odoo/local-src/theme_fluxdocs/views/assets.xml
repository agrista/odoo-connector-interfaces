<?xml version="1.0" encoding="utf-8" ?>
<openerp>
	<data>
		<template id="mystyle" name="My style" inherit_id="website.assets_frontend">
			
			<!-- CSS -->
			<xpath expr="link[@href='/website/static/src/css/website.css']" position="replace">
				<link href="/theme_fluxdocs/static/less/webfonts.css" rel="stylesheet" type="text/less" />
				<link href="/theme_fluxdocs/static/less/style.less" rel="stylesheet" type="text/less" />
			</xpath>
			
			
 			<!-- JS & Favicon -->
			<xpath expr="." position="inside">
				<link rel="shortcut icon" href="/theme_fluxdocs/static/favicon/favicon_v02.png" type="image/x-icon" />
				<link rel="icon" href="/theme_fluxdocs/static/favicon/favicon_v01.png" type="image/x-icon" />
				<script type="text/javascript" src="/theme_fluxdocs/static/js/fluxdock.js"></script>
			</xpath>
			
			
		</template>
		
	</data>
</openerp>