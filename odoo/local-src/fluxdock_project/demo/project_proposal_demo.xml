<odoo>

<record model="res.partner.category" id="industry_architecture">
  <field name="name">Architecture</field>
</record>
<record model="res.partner.category" id="industry_acoustic">
  <field name="name">Acoustic</field>
</record>
<record model="res.partner.category" id="industry_graphic_design">
  <field name="name">Graphic Design</field>
</record>
<record model="res.partner.category" id="industry_industry_and_product_design">
  <field name="name">Industry and Product Design</field>
</record>
<record model="res.partner.category" id="industry_interior_design">
  <field name="name">Interior Design</field>
</record>
<record model="res.partner.category" id="industry_scenography">
  <field name="name">Scenography</field>
</record>

<record model="project.proposal" id="project_proposal_1">
  <field name="name">Proposal 1</field>
  <field name="website_short_description">Teaser 1</field>
  <field name="website_published" eval="1"/>
  <field name="create_uid" ref="base.user_demo"></field>
  <field name="country_id" ref="base.ch"/>
  <field name="industry_ids" eval="[(6,0,[ref('fluxdock_project.industry_architecture')])]"/>
  <field name="expertise_ids" eval="[(6,0,[ref('partner_project_expertise.expertise_business_planning')])]"/>
</record>

<record model="project.proposal" id="project_proposal_2">
  <field name="name">Proposal 2</field>
  <field name="website_short_description">Teaser 2</field>
  <field name="website_published" eval="1"/>
  <field name="create_uid" ref="base.user_demo"></field>
  <field name="country_id" ref="base.ch"/>
  <field name="industry_ids" eval="[(6,0,[ref('fluxdock_project.industry_scenography')])]"/>
  <field name="expertise_ids" eval="[(6,0,[ref('partner_project_expertise.expertise_branding')])]"/>
</record>

<record model="project.proposal" id="project_proposal_3">
  <field name="name">Proposal 3</field>
  <field name="website_short_description">Teaser 3</field>
  <field name="website_published" eval="1"/>
  <field name="create_uid" ref="base.user_demo"></field>
  <field name="country_id" ref="base.ch"/>
  <field name="industry_ids" eval="[(6,0,[ref('fluxdock_project.industry_acoustic'), ref('fluxdock_project.industry_interior_design'), ref('fluxdock_project.industry_graphic_design')])]"/>
  <field name="expertise_ids" eval="[(6,0,[ref('partner_project_expertise.expertise_branding'), ref('partner_project_expertise.expertise_business_planning'), ref('partner_project_expertise.expertise_innovation_design')])]"/>
</record>

<record model="project.proposal" id="project_proposal_iart_1">
  <field name="name">Proposal iart 1</field>
  <field name="website_short_description">Teaser iart 1</field>
  <field name="website_published" eval="1"/>
  <field name="create_uid" ref="base.user_root"></field>
  <field name="country_id" ref="base.ch"/>
  <field name="industry_ids" eval="[(6,0,[ref('fluxdock_project.industry_scenography')])]"/>
  <field name="expertise_ids" eval="[(6,0,[ref('partner_project_expertise.expertise_branding')])]"/>
</record>

